#!/usr/bin/execlineb -P

ifte
{
  touch
    /etc/services.d/pulpcore-worker@1/down
    /etc/services.d/pulpcore-worker@2/down
}
{
  touch
    /etc/services.d/new-pulpcore-worker@1/down
